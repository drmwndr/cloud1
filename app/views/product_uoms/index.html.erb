<h1>Listing product_uoms</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Uom</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_uoms.each do |product_uom| %>
  <tr>
    <td><%= product_uom.description %></td>
    <td><%= product_uom.uom %></td>
    <td><%= link_to 'Show', product_uom %></td>
    <td><%= link_to 'Edit', edit_product_uom_path(product_uom) %></td>
    <td><%= link_to 'Destroy', product_uom, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

